<h1>New Article</h1>

<%= form_with scope: :user, url: users_path, local: true do |form| %>
  <table>
    <tr><td><%= form.label :first_name %></td><td><%= form.label :last_name %></td></td>
    <tr><td><%= form.text_field :first_name %></td><td><%= form.text_field :last_name %></td></td>
  </table> 
  <p>
    <%= form.submit %>
  </p>
<% end %>